{
  "name": "@jujulego/injector",
  "version": "0.1.5",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "./dist",
    "./dynamic"
  ],
  "imports": {
    "#/current-scope": {
      "types": "./dynamic/current-scope.d.ts",
      "node": "./dynamic/current-scope.node.js",
      "default": "./dynamic/current-scope.js"
    }
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:types": "vitest typecheck"
  },
  "dependencies": {
    "@jujulego/utils": "^3.0.4",
    "@swc/helpers": "^0.5.3",
    "kyrielle": "^0.7.0"
  },
  "devDependencies": {
    "@jujulego/jill": "2.4.0",
    "@jujulego/vite-plugin-swc": "1.1.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.100",
    "@types/node": "18.19.1",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.54.0",
    "eslint-plugin-vitest": "0.3.10",
    "shx": "0.3.4",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.2"
}

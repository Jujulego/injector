{
  "name": "@jujulego/injector",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./tests": {
      "types": "./dist/tests/index.d.ts",
      "import": "./dist/tests/index.js",
      "default": "./dist/tests/index.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:types": "vitest typecheck"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@jujulego/aegis": "^2.0.1",
    "@jujulego/utils": "^2.0.2"
  },
  "devDependencies": {
    "@jujulego/jill": "2.3.3",
    "@jujulego/vite-plugin-swc": "1.1.0",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.96",
    "@types/node": "18.18.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.53.0",
    "eslint-plugin-vitest": "0.3.9",
    "reflect-metadata": "0.1.13",
    "shx": "0.3.4",
    "typescript": "5.2.2",
    "vite": "5.0.0-beta.17",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.1"
}

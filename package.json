{
  "name": "@jujulego/injector",
  "version": "0.1.5",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "tests.d.ts"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tests": {
      "types": "./dist/tests/index.d.ts",
      "default": "./dist/tests/index.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:types": "vitest typecheck"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.3",
    "kyrielle": "^0.6.0"
  },
  "devDependencies": {
    "@jujulego/jill": "2.4.0",
    "@jujulego/vite-plugin-swc": "1.1.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@types/node": "18.18.13",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.54.0",
    "eslint-plugin-vitest": "0.3.10",
    "reflect-metadata": "0.1.13",
    "shx": "0.3.4",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.2"
}
